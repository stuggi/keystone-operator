{
    "command": "/usr/sbin/httpd",
    "config_files": [
        {
            "source": "/var/lib/config-data/merged/keystone.conf",
            "dest": "/etc/keystone/keystone.conf",
            "owner": "keystone",
            "perm": "0600"
        },
        {
            "source": "/var/lib/config-data/merged/custom.conf",
            "dest": "/etc/keystone/keystone.conf.d/custom.conf",
            "owner": "keystone",
            "perm": "0600"
        },
        {
            "source": "/var/lib/config-data/merged/httpd.conf",
            "dest": "/etc/httpd/conf/httpd.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "/var/lib/config-data/merged/ssl.conf",
            "dest": "/etc/httpd/conf.d/ssl.conf",
            "owner": "root",
            "perm": "0644"
        },
        {
            "source": "/var/lib/config-data/tls/certs/*",
            "dest": "/etc/pki/tls/certs/",
            "owner": "root",
            "perm": "0640",
            "optional": true,
            "merge": true
        },
        {
            "source": "/var/lib/config-data/tls/private/*",
            "dest": "/etc/pki/tls/private/",
            "owner": "root",
            "perm": "0600",
            "optional": true,
            "merge": true
        },
        {
            "source": "/var/lib/credential-keys",
            "dest": "/etc/keystone/",
            "owner": "keystone:keystone",
            "perm": "0700"
        },
        {
            "source": "/var/lib/fernet-keys",
            "dest": "/etc/keystone/",
            "owner": "keystone:keystone",
            "perm": "0700"
        }
    ]
}
